{
  "name": "playwright-server",
  "version": "1.0.0",
  "description": "A minimalistic Node.js server with TypeScript",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "filter-jobs": "ts-node-dev src/scripts/filterNonITJobs.ts",
    "get-job-titles": "ts-node-dev src/scripts/getAllJobTitles.ts",
    "remove-finance-jobs": "ts-node-dev src/scripts/removeFinanceJobs.ts",
    "test-non-it-removal": "ts-node-dev src/scripts/testNonITRemoval.ts",
    "scrape-swissdevjobs": "ts-node-dev src/scripts/scrapeSwissDevJobs.ts",
    "debug-swissdevjobs": "ts-node-dev src/scripts/debugSwissDevJobs.ts",
    "update-swissdevjobs": "ts-node-dev src/scripts/updateSwissDevJobsCompanyLocation.ts",
    "rescrape-swissdevjobs": "ts-node-dev src/scripts/removeAndRescrapeSwissDevJobs.ts",
    "scrape-swissdevjobs-desc": "ts-node-dev src/scripts/rescrapeSwissDevJobsWithDescriptions.ts",
    "populate-tech-roles": "ts-node-dev src/scripts/populateTechnologyRoles.ts",
    "check-tech-roles": "ts-node-dev src/scripts/checkTechnologyRoleMappings.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.50.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "node-cron": "^4.1.0",
    "playwright": "^1.40.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/node": "^20.0.0",
    "@types/node-cron": "^3.0.11",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
